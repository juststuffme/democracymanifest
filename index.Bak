        document.getElementById('telegramBtn').href = '#'; // Add your Telegram link
        document.getElementById('twitterLink').href = '#'; // Add your Twitter link
        document.getElementById('telegramLink').href = '#'; // Add your Telegram link
        document.getElementById('dexscreenerLink').href = 'https://dexscreener.com/solana/42JxP81kckK4hv6rS44RXGXs9PVGjaCT6ggyAzUzC';

        // Smooth scroll
        document.querySelectorAll('a[href^="#"]').forEach(anchor => {
            anchor.addEventListener('click', function (e) {
                e.preventDefault();
                const target = document.querySelector(this.getAttribute('href'));
                if (target) {
                    target.scrollIntoView({ behavior: 'smooth' });
                }
            });
        });
